{
	"env": {
		"es2022": true,
		"browser": false,
		"node": true
	},
	"extends": ["standard", "prettier"],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["@typescript-eslint"],
	"rules": {
		"@typescript-eslint/no-unused-vars": "error"
	},
	"globals": {
		"logger": "writable",
		"NodeJS": "readonly"
	}
}
